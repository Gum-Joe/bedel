{
  "name": "bedel",
  "version": "0.1.0",
  "description": "Build, ship and manage apps, all from one dashboard.",
  "main": "app.js",
  "scripts": {
    "start": "node bin/bedel server --color",
    "dev": "nodemon --exec \"nodejs-dashboard --\" --ignore client --ignore build -- bin/bedel server --color --no-prompt --debug",
    "compile": "gulp compile",
    "compile:sass": "gulp compile:sass",
    "install": "npm run mkdirs && npm run build:semantic",
    "build:semantic": "cd client/semantic && gulp build",
    "build": "gulp webpack",
    "lint": "eslint .",
    "mkdirs": "node script/mkdirs",
    "nodemon-webpack": "webpack-dashboard -c red -- nodemon --ignore client --ignore build bin/bedel server --color --debug",
    "nodemon": "nodemon --ignore client --ignore build bin/bedel server --color --debug",
    "test": "mocha",
    "coveralls": "istanbul cover -x app/util/logger.js ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gum-Joe/bedel.git"
